<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="GUI">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Label" id="Title">
        <property name="font">{Ubutnu} 24 {}</property>
        <property name="padx">100</property>
        <property name="text" translatable="yes">MailWatcher Manager</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_currentpass">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="padx">0</property>
        <property name="text" translatable="yes">Current password:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">15</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="tb_currentpass">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="state">readonly</property>
        <property name="textvariable">string:serverpass</property>
        <property name="width">40</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">15</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_passchange">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Password Change:
</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">16</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="tb_newpass">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="insertontime">0</property>
        <property name="textvariable">string:newpass</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">16</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btn_newpassconfirm">
        <property name="command">changepass</property>
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Confirm</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">16</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_currentport">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Current port:</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">17</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="tb_currentport">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="selectborderwidth">0</property>
        <property name="state">readonly</property>
        <property name="textvariable">string:currentport</property>
        <property name="width">40</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">17</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_changeport">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Port Change:
</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">18</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="tb_changeport">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="textvariable">string:newport</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">18</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btn_portconfirm">
        <property name="command">changeport</property>
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Confirm</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">18</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lblcurrentcmdpath">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="pady">20</property>
        <property name="text" translatable="yes">Current command:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">19</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="tb_currentcmd">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="state">readonly</property>
        <property name="textvariable">string:fullcmd</property>
        <property name="width">40</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">19</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_changecmd">
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Change command:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">20</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="pygubu.builder.widgets.pathchooserinput" id="pc_startdir">
        <property name="textvariable">string:newcmd</property>
        <property name="type">file</property>
        <layout>
          <property name="ipadx">0</property>
          <property name="propagate">True</property>
          <property name="row">20</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btn_cmdconfirm">
        <property name="command">changecmd</property>
        <property name="font">{Ubuntu} 12 {}</property>
        <property name="text" translatable="yes">Confirm</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">20</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
